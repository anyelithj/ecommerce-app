    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.6.2",
    "standard": "17.0.0",
    "vite": "^2.9.9"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-jsx",
      "prettier"
